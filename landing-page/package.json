{
  "name": "landing-page",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npm run build && npx wrangler pages deploy dist --project-name=every-app-landing-page",
    "types:check": "astro sync && tsc --noEmit",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/starlight": "^0.29.3",
    "@astrojs/tailwind": "^5.1.2",
    "astro": "^4.16.18",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.7.2",
    "wrangler": "^4.45.4"
  }
}
